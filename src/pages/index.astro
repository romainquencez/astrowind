    isAfterContent={true}
    isReversed
    columns={2}
    items={[
      {
        title: 'Python',
        icon: 'tabler:brand-python',
        classes: {icon: 'bg-python'},
      },
      {
        title: 'Django',
        icon: 'tabler:brand-django',
        classes: {icon: 'bg-django'},
